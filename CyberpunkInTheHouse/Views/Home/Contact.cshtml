@model IEnumerable<CyberpunkInTheHouse.Models.Client>
@{
    ViewBag.Title = "Personal";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="table-responsive">
    <table class="table table-hover table-condensed">
        <thead class="">
            <tr>
                <th>First name</th>
                <th>Last name</th>
                <th>Email</th>
                <th>Telephone</th>
                <th>Adres</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var client in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => client.FirstName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => client.LastName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => client.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => client.Telephone)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => client.Adres)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

